{
  "name": "Scale Min-Max",
  "description": "Rescales data to the range [0, 1].",
  "tags": [
    "data processing",
    "scaling",
    "preprocessing"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/scale_min_max_app"
  ],
  "container": "ghcr.io/tercen/scale_min_max_app:specs",
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Variable",
            "type": "",
            "description": "Variable identifiers",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Observation",
            "type": "",
            "description": "Observation identifiers",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement values to be scaled",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}